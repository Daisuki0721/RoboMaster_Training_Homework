{
    "version": 8,
    "configurePresets": [
        {
            "name": "release",
            "displayName": "GCC 15.2.0 x86_64-w64-mingw32",
            "description": "Using compilers: C = c:\\Compiler\\mingw64\\bin\\gcc.exe, CXX = c:\\Compiler\\mingw64\\bin\\g++.exe",
            "generator": "MinGW Makefiles",
            "binaryDir": "${sourceDir}/out/build/${presetName}",
            "cacheVariables": {
                "CMAKE_INSTALL_PREFIX": "${sourceDir}/out/install/${presetName}",
                "CMAKE_C_COMPILER": "c:/Compiler/mingw64/bin/gcc.exe",
                "CMAKE_CXX_COMPILER": "c:/Compiler/mingw64/bin/g++.exe",
                "CMAKE_BUILD_TYPE": "Debug"
            }
        }
    ]
}